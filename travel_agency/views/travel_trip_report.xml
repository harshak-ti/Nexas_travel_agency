<odoo>

    <template id="travel_trip_report_template">

        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <t t-call="web.external_layout">
                    <div class="page">

                        <t t-esc="o.name"/>
                        <!-- <t t-esc="data_image_uri('o.image_1920')"/> -->
                        <img t-att-src="image_data_uri(o.image_1920)" style="width:100px;height:100px;"/>
                    </div>
                </t>
            </t>
        </t>
    </template>

</odoo>