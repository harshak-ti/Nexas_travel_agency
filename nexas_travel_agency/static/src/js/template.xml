<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <t t-name="FilteredSelectionFieldTemplate">
        <select t-model="props.value">
            <t t-foreach="filteredOptions" t-as="option" t-key="option[0]">
                <option t-att-value="option[0]" t-esc="option[1]"></option>
            </t>
        </select>
    </t>


</odoo>
