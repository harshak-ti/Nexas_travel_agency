<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Form View for Travel Supplier -->


    <!-- Tree View for Travel Supplier -->
    <record id="view_choice_tree" model="ir.ui.view">
        <field name="name">choice.tree</field>
        <field name="model">choice</field>
        <field name="arch" type="xml">
            <tree string="Choice" editable="top">
                <field name="name"/>

            </tree>
        </field>
    </record>


    <record id="action_choice" model="ir.actions.act_window">
        <field name="name">Choices</field>
        <field name="res_model">choice</field>
        <field name="view_mode">tree</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create
            </p>
        </field>
    </record>

</odoo>